{
  "name": "countee",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@types/expo": "30.0.1",
    "@types/expo__vector-icons": "6.2.3",
    "@types/jest": "23.3.0",
    "@types/react": "^16.4.18",
    "@types/react-intl": "^2.3.11",
    "@types/react-native": "0.57.12",
    "@types/react-navigation": "2.13.3",
    "@types/react-test-renderer": "^16.0.3",
    "jest-expo": "28.0.0",
    "react-test-renderer": "^16.6.0",
    "ts-jest": "23.0.1",
    "tslib": "^1.9.3",
    "tslint": "5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "3.1.6"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "lint": "tslint src/**/*.ts",
    "start": "expo start",
    "validate": "npm run lint --scripts-prepend-node-path && tsc --noEmit"
  },
  "dependencies": {
    "expo": "31.0.5",
    "firebase": "^5.5.7",
    "intl": "^1.2.5",
    "native-base": "^2.8.1",
    "react": "16.5.0",
    "react-intl": "^2.7.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.1.tar.gz",
    "react-navigation": "^2.18.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
